# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp

# Ignore IntelliJ
.idea

# Ignore byebug history
.byebug_history

# Ignore DS_Store files
.DS_Store

# Npm
/node_modules
npm-debug.log*
node_modules
package-lock.json

# Ignore local files
*.local
